.root_container{position: relative;}
.team_info{width: 95%;height: 85%;background-color: rgba(255,255,255,.4);z-index: 99;position: absolute;left:0; top:0;right:0;bottom:0;margin:12% 2.5%;display: flex;flex-direction: column;align-items: center; border-radius: 0.2rem;box-sizing: border-box;}
.team_info .team_info_title{width:100%;height: 0.8rem;background: #0F2E42;text-align: center;position: relative;color: #7091A6;line-height: 0.8rem;font-size: 0.3rem;border-radius: 0.2rem 0.2rem 0 0 ;}
.team_info .team_info_title img{position: absolute;top: 0.15rem;right: 0.15rem;width: 0.3rem;}
.team_info .info_list{background-color: rgb(11,53,79);width: 100%;overflow: scroll;height: 75%;}
.team_info .info_list li{display: flex; justify-content: space-around;align-items: center;width: 100%;height: 1rem;}
.team_info .info_list li .left{width: 50%;display: flex;align-items: center;}
.team_info .info_list li .left img{height: 0.8rem; border-radius: 0.2rem;margin-right: 0.1rem;margin-left: 0.1rem;}
.team_info .info_list li .player_cardname{width: 25%;}
.team_info .info_list li:nth-child(even){background-color: rgb(7,47,69);}
.team_info .info_list li div{font-size: 0.25rem;overflow:hidden;text-overflow: ellipsis;white-space: nowrap;}
.team_info .info_list li .coin{display: flex;align-items: center;width: 20%;}
.team_info .info_list li .coin img{width: 0.24rem;margin-right: 0.1rem;}


.team_info .salary_info{display: flex;flex-direction: column;height: 1.2rem;background-color: rgba(15,50,70,.8);}
.team_info .salary_info .con:first-child{margin-bottom: -0.2rem;margin-top: 0.1rem;}
.team_info .salary_info .con{display: flex;justify-content: space-around;height: 50%;align-items: center;}
.team_info .salary_info .con .con_info{display: flex;width: 50%;margin-left: 0.3rem;}
.team_info .salary_info .con .con_info div{font-size: 0.22rem;color: #428A9E;font-weight: bold;}

.team_info .button{height: 1rem; background: rgba(17,49,70,.9);text-align: center;font-size: 0.3rem;color: #EBB505;letter-spacing: 0.05rem;border-radius:  0 0 0.2rem 0.2rem ;display: flex;justify-content: center;align-items: center;}


.message{position: absolute;top: 0;left: 0;right: 0;bottom: 0;margin: auto;display: flex;flex-direction: column;align-items: center; border-radius: 0.2rem;width: 90%;height: 25%;background-color: #0D354F;}
.message .message_con{width: 85%;height: 60%;display: flex; font-size: 0.3rem;color: #80A4BB;align-items: center;padding-top: 0.3rem;letter-spacing: 0.03rem;}
.message .buttons{height: 30%;background: url(../images/createteam/text_bg.png) no-repeat;background-size:100% 100%;display: flex;justify-content: space-around;align-items: center;}
.message .buttons div{font-size: 0.28rem;color: #FEC000;}
.message .buttons .line{width: 0.02rem;height:97%;background-color: #386D8F;}


.message .message_con1{display: flex;flex-direction: column;width: 80%;height: 60%;font-size: 0.24rem;color: #80A4BB;align-items: center;padding-top: 0.5rem;box-sizing: border-box;}
.message .message_con1 div:first-child{font-size: 0.3rem;color: #83A6BE;}
.message .message_con1 div:nth-of-type(2){color: #49697E;margin-top: 0.1rem;}
.message .button{height: 30%;background: url(../images/createteam/text_bg.png) no-repeat;background-size:100% 100%;margin-top: 0.3rem;color: #FEC000;text-align: center;font-size: 0.3rem;letter-spacing: 0.05rem;padding-top: 0.2rem;box-sizing: border-box;border-radius: 0 0 0.2rem 0.2rem;}

.message .success{padding-top: 0.28rem;}
.message .success img{margin-bottom: 0.1rem;width: 0.8rem;}
.message .button{height: 30%;background: url(../images/createteam/text_bg.png) no-repeat;background-size:100% 100%;display: flex;justify-content: space-around;margin-top: 0.3rem;padding-top: 0;line-height: 1rem;}
.message .button div{width: 50%;display: flex;justify-content: center;align-items: center;}
.message .button div a{font-size: 0.26rem;color: #FEC000;}
.message .button .line{width: 1px;height:97%;background-color: #386D8F;}


.recharge{position: absolute;top: 0;left: 0;right: 0;bottom: 0;margin: auto;display: flex;flex-direction: column;align-items: center; border-radius: 0.2rem;width: 82%;height: 5.6rem;background-color: #0D354F;}
.recharge .text{width: 100%;height: 2rem;text-align: center;font-size: 0.28rem;color: #8CB1C8;padding: 0.6rem 0.8rem 0rem 0.6rem;box-sizing: border-box;}
.recharge .recharge_list{width: 100%; flex: 1;}
.recharge .recharge_list li{height: 25%;text-align: center;font-size: 0.3rem;color: #ffc000;display: flex;align-items: center;justify-content: center;box-sizing: border-box;border-bottom: 0.02rem solid #3B7195;min-height: 0.9rem;box-sizing: border-box;}
.recharge .recharge_list li:first-child{border-top: 0.02rem solid #3B7195;}
.recharge .recharge_list li:last-child{border-radius: 0 0 0.2rem 0.2rem;}
