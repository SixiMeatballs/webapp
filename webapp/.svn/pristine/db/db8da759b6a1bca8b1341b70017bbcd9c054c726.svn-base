.selected_player_list{display: flex; flex-direction: row;min-height: 1.2rem;height:1.2rem; background:#124664; border-top:solid 0.01rem #052232;border-bottom:solid 0.01rem #052232; align-items: center;}
.selected_player_list .selected_statistics{width:2.2rem; height:100%; background:#163545; font-size:0.3rem; display:flex;align-items: center;}
.selected_player_list .selected_statistics div{display:flex; flex-direction: column; align-items: center; width:100%;color: #7DA0B6;font-size: 0.25rem;}
.selected_player_list .player_selected_list{overflow:scroll;height:100%;}
.selected_player_list .player_selected_list ul{height:100%;display: flex; flex-direction: row; align-items: center; justify-content: space-between;}
.selected_player_list .player_selected_list ul li{ display: flex;flex-direction: row-reverse;margin: 0.1rem;box-sizing: border-box;}
.selected_player_list .player_selected_list ul li .player_delete{margin-left: -0.2rem;z-index: 2;width: 0.36rem;height: 0.36rem;display: flex;}
.selected_player_list .player_selected_list ul li .player_delete img{width: 0.36rem;height: 0.36rem;}
.selected_player_list .player_selected_list ul li img{border-radius: 50%;width: 0.86rem;height: 0.86rem;}

.country_list{border-top:solid 0.01rem #093f5e; border-bottom:solid 0.01rem #1b6289; height:0.6rem; min-height:0.6rem; background-color:#0e2d3d; width:100%; overflow:auto;}
.country_list ul{height:100%;  display:flex;align-items: center; flex-wrap:nowrap; margin:0 5%;}
.country_list ul li{margin-left:0.3rem;}
.country_list ul li .country_item{display: flex;align-items: center;}
.country_list ul li .countryName{ flex-wrap:nowrap; letter-spacing: 0.05rem; text-align: center;overflow: hidden; text-overflow:ellipsis;white-space:nowrap;max-width:1.4rem;font-size: 0.26rem;color: #C1C4C8;}
.country_list ul li .selectedCount{margin-top: -0.3rem;background:#dd3500;width:0.25rem;height:0.25rem;border-radius: 50%;color:white;text-align: center;line-height: 0.25rem; font-size:0.14rem;vertical-align: middle;}
.country_list ul .activated{color:#FEC201;}
.country_list ul .activated .countryName{font-size:0.28rem;color:#FEC201;}

.player_list {width:100%;min-height: 5.5rem;max-height: 7.5rem;padding-top: 0.1rem ; overflow:scroll;}
.player_list ul{display:flex; width:100%; flex-wrap: wrap; padding-bottom:1.5rem;}
.player_list .content li{width:30%; margin:1.5%; border-radius:0.2rem; background-color:#0f2e3e;display:flex; flex-direction:column;height: 3.5rem;min-height: 3.5rem;}
.player_list .content  li .player_name{color: #AFB7BA;text-align: center;line-height: 0.35rem;font-size: 0.24rem;letter-spacing:0.03rem ;border-radius: 0.2rem 0.2rem 0 0;background-color: #0D2434;overflow:hidden;text-overflow: ellipsis;white-space: nowrap;height: 0.7rem;padding: 0 0.07rem;}
.player_list li .container{position:relative;height: 2.6rem;min-height: 2.6rem;}
.player_list li .container .player_position{text-align:center; width:0.3rem; height:0.8rem; top:0.3rem; right:0.1rem; position:absolute;}
.player_list li .container .forward{background:url(../images/selectplayer/forward.png); background-repeat:no-repeat; background-size: 100% 100%;}
.player_list li .container .player_pic{display:flex; flex-direction:column; align-items: center;height: 100%;}
.player_list li .container .player_pic img{width: 1.5rem;margin-top: 0.2rem;margin-right: 0.2rem;border-radius: 0.2rem;}
.player_list li .player_button{font-weight: bold; text-align: center;height: 1rem;font-size: 0.25rem;letter-spacing: 0.03rem;color: #FEBF00;background-color: #091B27;border-radius: 0 0 0.2rem 0.2rem ;display: flex;justify-content: center;align-items: center;}
.player_list ul .player_unselected .player_button{ color: #fdc201;}
.player_list ul .player_selected{ box-shadow: 0px 0px 5px #FFF;}
.player_list ul .player_selected .player_button{ color: #285b74;}
.player_list li .container .player_salary{ position:absolute; bottom:0; background:rgba(0,0,0,0.2); background-repeat:no-repeat; width:100%; display:flex; align-items:center;height:0.5rem;min-height:0.5rem;}
.player_list li .container .player_salary div{margin-left:0.1rem;display: flex;}
.player_list li .container .player_salary  .gold_icon img{width: 0.24rem;height: 0.24rem;}

.player_list li .container .player_salary .deleted{font-size:0.16rem;text-decoration:line-through;}
.player_list li .container .player_salary .current{font-size:0.2rem; color:#ffCC00; }
.player_list li .container .player_card_list .current_card{ position:absolute; right:0; bottom:0.5rem;  min-width:1rem; max-width:1rem; height:0.4rem; line-height: 0.4rem;  background:#2485c2; color:#D9E7F2; border-radius: 0.3rem 0 0 0.3rem; font-size:0.2rem;padding-left:0.1rem; overflow: hidden; text-overflow:ellipsis;white-space:nowrap;}

.player_list li .container .player_card_popup_panel{position:absolute; background-color:#2485c2;  padding:0.1rem 0;  z-index:5; color:#EEE; display:flex; border-radius:0.15rem; width:100%; top:2.6rem;}
.player_list li .container .player_card_popup_panel ul{list-style:none;background-color:#2485c2;  padding:0; margin:0; display:flex; flex-direction: column;}
.player_list li .container .player_card_popup_panel ul .player_card{width:98%; background-color:#2485c2; font-size:0.2rem; height:0.5rem!important;min-height:0.5rem!important;display:flex; flex-direction: row; align-items: center;border-radius: 0;}
.player_list li .container .player_card_popup_panel ul li:nth-child(even){background-color:#1b77af;}
.player_list li .container .player_card_popup_panel ul .player_card input[type='radio']{ width:0.2rem; height:0.2rem; margin:0 0.1rem;}
.player_list li .container .player_card_popup_panel ul .player_card div{font-size: 0.16rem;}

.fix_bottom{margin-top:-0.3rem;width: 100%;height: 3.2rem;}

