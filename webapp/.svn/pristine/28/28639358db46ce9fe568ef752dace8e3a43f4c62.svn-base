.header_bg{width:100%;height:3.45rem;min-height:3.45rem;background: url(../images/prize/prize_bg.png) no-repeat;background-size: 100% 100%;}
.header_bg .back_icon{padding: 0.3rem;display: flex;align-items: center;}
.header_bg .back_icon img{width: 0.72rem;}
.header_bg .back_icon a{margin-left: 4.9rem; font-size: 0.3rem;color: #ccc;}

.prize_title{width: 100%;height: 0.85rem;min-height: 0.85rem;background: rgb(12,52,78);display: flex; justify-content: space-around;align-items: center;}
.prize_title div{width: 50%;height: 100%;line-height: 0.85rem;text-align: center;font-size: 0.3rem;color: #E2E4E7;letter-spacing: 0.02rem;}
.prize_title .active{border-bottom: 0.04rem solid #fff;box-sizing: border-box;}


.paize_content{display: flex;justify-content: flex-start;width: 100%;overflow: scroll;padding-top: 0.054rem; height:100%;}

.paize_content ul{overflow:scroll;}
.paize_content .round{background-color: rgba(18,65,95,.75);}
.paize_content .round li{width: 1.2rem;height: 1rem;border-bottom: 0.01rem solid #144B6E;display: flex;flex-direction: column;align-items: center;background-color: #19608E;padding-top: 0.12rem;box-sizing: border-box;}
.paize_content .round li div{font-size: 0.28rem;color: #fff;letter-spacing: 0.02rem;}
.paize_content .round  .activated div{color: #6E92AD;}
.paize_content .round  .activimg{background-color: #1E79B2;}

.paize_content .paize_list{flex-grow: 1;background-color: rgba(15,69,110,.75);}
.paize_content .paize_list li{display: flex;justify-content: space-around;height: 1rem;align-items: center;}
.paize_content .paize_list li .left{display: flex;justify-content: space-around;width: 50%;align-items: center;}
.paize_content .paize_list li .left img{width: 0.5rem;}
.paize_content .paize_list li .left .rank{width: 1.5rem;}

.paize_content .paize_list li .right{width: 45%;display: flex;justify-content: center;}



.paize_content .paize_list li div{font-size: 0.22rem;color: #fff;letter-spacing: 0.02rem;}
.paize_content .paize_list li:nth-of-type(even){background-color: rgba(15,69,99,.8);}


div[prizetype='day'] .round{display: block;}
div[prizetype='total'] .round{display: none;}
