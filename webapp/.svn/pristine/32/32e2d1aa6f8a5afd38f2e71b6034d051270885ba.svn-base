*{padding:0; margin:0;border:0;font-family: "helvetica";text-decoration:none;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}
html,body{height: 100%;}
body{background: url(../img/global/bg.png) repeat-y;background-size: 100% auto;}


.full_width{width:100%;}
.body-section{display: flex;flex-direction: column;}
.container{width: 100%;height: 100%;position: relative;background: url(../img/index/bg.png) no-repeat;background-size: 100% 100%;}

.header{height: 0.6rem;display: flex;justify-content: space-between;padding: 0 0.3rem;}
.header img{height: 0.6rem;}
.header a img{float: right;}
.header div{display: flex;margin-top: 0.14rem;margin-left: 0.2rem;float: left;}
.header a{display: block;float: right;}

.bottom{width: 100%;display: flex;justify-content: space-around; align-self:flex-end;margin-bottom: 0.56rem;}
.bottom a img{height: 1.6rem;}

.center{width:100%; height:100%;display: flex;justify-content: center;flex-direction: column;align-items: center;flex: 1;}
.center .center_panel{display: flex;justify-content: space-between;flex-direction: column;align-items: center; height:7rem;}
.center .thems img{height: 4.52rem}

.playgame  img{width: 100%;}