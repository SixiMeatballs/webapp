* {
    padding: 0;
    margin: 0;
    border: 0;
    font-family: "pingfang sc ";
    list-style-type: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-appearance: none;
    -webkit-overflow-scrolling: touch;
}

body,
html {
	width: 100%;
	height: 100%;
	overflow: auto;
	font-family: pingfang sc;
}

.container {
    display: flex;
    flex-direction: column;
    padding: 0.3rem;
    background: #fff;
}

.container .headtitle{
	font-size: 0.36rem;
	color: #333;
	text-align: center;
	margin-bottom: 0.3rem;
}
.container p{
	font-size: 0.28rem;
	color: #333;
	margin: 0.1rem 0;
	/*text-indent: 0.56rem;*/
}
.container p img{
	width: 100%;
	margin: 0.1rem 0;
}
.container a{
	font-size: 0.3rem;
	color: #ff0036;
	margin-top: 0.5rem;
}
.container h3{
	font-size: 0.28rem;
	color: #2293E4;
	margin-bottom: 0.3rem;
}
.subtitle{
	font-weight: bold;
}
#a1{
	margin-top: 0.5rem;
}
.container .blanks{
	width: 100%;
	height: 0.1rem;
}
.container .blank{
	width: 100%;
	height: 0.08rem;
	background: #eee;
	margin: 0.3rem 0;
}
.container .footer{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}

.subtitles{
	width: 100%;
	font-size: 0.3rem;
	text-align: center;
	margin: 0.3rem 0;
	background: #eee;
}
