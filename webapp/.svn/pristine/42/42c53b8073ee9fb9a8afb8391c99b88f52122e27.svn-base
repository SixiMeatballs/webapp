* {
	padding: 0;
	margin: 0;
	font-family: 'PingFang-SC-Regular';
}
a {
    text-decoration: none;
}
.container{
    overflow-x:hidden;
}
.container header {
    height: 1rem;
    display: flex;
    align-items: center;
    padding-left: 0.3rem;
    position:fixed;
    top:0;
    width:100%;
    background:#fff;
    z-index:9;
}
.container header a{
	margin-right: 0.1rem;
    display:flex;
}
.container header input{
	width: 5rem;
	height: 0.58rem;
	background-color: #f0f0f0;
	border-radius: 0.28rem;
	border: none;
	outline: none;
	font-size: 0.26rem;
	color: #999;
	padding-left: 0.6rem;
}
.container header span{
    font-size:0.3rem;
    color:#333;
    margin-left:0.3rem;
}

nav{
    position:fixed;
    top:1rem;
    background:#fff;
    width:100%;
    z-index:9;
}
.tabList{
	position: relative;
}
.listTab {
	font-size: 0.3rem;
	color:#333;
	padding-left: 0.3rem;
	display: flex;
	overflow-x: scroll;
}
.listTab .tabItem{
	width: 1rem;
	min-width: 1.2rem;
	height: 0.8rem;
	min-height: 0.8rem;
	text-align: center;
	margin: 0 0.1rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.listTab  .selected{
	color: #ff0036;
	border-bottom: 0.02rem solid #ff0036;
}
.moreListIcon{
	position: absolute;
	right: 0.3rem;
	bottom: 0.45rem;
    width:0.32rem;
    height:0.32rem;
    display:flex;
}
.moreList {
    position: absolute;
    top: 0.82rem;
    width: 100%;
    height: 4.66rem;
    background: #fff;
    overflow: scroll;
}
.moreList .itemList{
	display: flex;
	flex-wrap: wrap;
	font-size: 0.28rem;
	color: #333333;
}
.moreList .itemList .moreItem{
	width: 2rem;
	height: 0.58rem;
	border: 0.01rem solid #eee;	
	border-radius: 0.06rem;
	margin: 0.3rem 0.2rem;
	text-align: center;
	line-height: 0.58rem;
}
.moreList .itemList .selected{
	border: 0.01rem solid #DF3E3F;	
}


.blank {
	height: 0.23rem;
	background: #F0F0F0;
    margin-top:1.8rem;
}
.listContent{
	padding-left: 0.3rem;
}

.listContent .listCard{
	border-bottom: 0.01rem solid #eee;
	margin-top: 0.3rem;
}
.listContent .listCard .listCardHead{
	font-size: 0.3rem;
	color: #333;
	margin-bottom: 0.3rem;
}
.listContent .listCard .listCardContent{
	display: flex;
	justify-content: space-around;
}
.listContent .listCard  .headItem{
	width: 66.67%;
	float: left;
	margin: 0;
}
.listContent .listCard .listCardContent .item{
	display: flex;
	margin-right: 0.3rem;
}
.listContent .listCard .listCardContent .item img{
	width: 2.2rem;
	height: 1.5rem;
}
.listContent .listCard .listCardContent .item-3{
	width: 33.33%;
	margin: 0 0 0.18rem;
}
.listContent .listCard .listCardContent .item-3 img{
	width: 2.2rem;
	height: 1.5rem;
}
.listContent .listCard .listCardContent .item-1{
	width: 100%;
	padding-right: 0.3rem;
	margin: 0 0 0.18rem;
}
.listContent .listCard .listCardContent .item-1 img{
	width: 100%;
	height: 3rem;
}
.listContent .listCard .listCardContent .item-1 video{
    width:7rem;
    height:3.45rem;
}
.listContent .listCard .listCardFooter{
	display: flex;
	margin-bottom: 0.3rem;
}
.listContent .listCard .listCardFooter span{
	font-size: 0.24rem;
	color: #999;
}
.listContent .listCard .listCardFooter .form{
	margin-right: 0.3rem;
}




.noData {
    height: 10rem;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.noData img{
    width:2.4rem;
    height:1.38rem;
}
.noData span{
    font-size:0.3rem;
    color:#999;
    margin-top:0.5rem;
}