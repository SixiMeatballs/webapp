.selected_player_list{display: flex; flex-direction: row;min-height: 10rem;height:10rem; background:#124664; border-top:solid 1px #052232;border-bottom:solid 1px #052232; align-items: center;}
.selected_player_list .selected_statistics{width:15.16rem; height:100%; background:#163545; font-size:2rem; display:flex; flex-direction: row; align-items: center;}
.selected_player_list .selected_statistics div{display:flex; flex-direction: column; align-items: center; width:100%;}
.selected_player_list .player_selected_list{overflow:scroll;height:100%;}
.selected_player_list .player_selected_list ul{height:100%;display: flex; flex-direction: row; align-items: center; justify-content: space-between;}
.selected_player_list .player_selected_list ul li{ margin:1rem;display: flex;flex-direction: row-reverse;}
.selected_player_list .player_selected_list ul li .player_delete{margin-left: -2rem;z-index: 2;}
.selected_player_list .player_selected_list ul li img{border-radius: 50%;}

.country_list{border-top:solid 1px #093f5e; border-bottom:solid 1px #1b6289; height:6rem; min-height:6rem; background-color:#0e2d3d; width:100%; overflow:auto;}
.country_list ul{height:100%; width:90%; display:flex;align-items: center; font-size:2.3rem; flex-wrap:nowrap; margin:0 5%;}
.country_list ul li{margin-left:2rem;}
.country_list ul li .country_item{width:15rem;display: flex;align-items: center;}
.country_list ul li .countryName{ flex-wrap:nowrap; letter-spacing: 0.4rem; text-align: center;overflow: hidden; text-overflow:ellipsis;white-space:nowrap;max-width:13rem;}
.country_list ul li .selectedCount{margin-top: -1.5rem;background:#dd3500;width:2.1rem;height:2.1rem;border-radius: 50%;color:white;text-align: center;line-height: 2.1rem; font-size:1.4rem;vertical-align: middle;}
.country_list ul .activated{color:yellow;}
.country_list ul .activated .countryName{font-size:2.2rem;}

.player_list {width:100%;min-height: 40rem;max-height: 100rem;padding-top: 1rem ; overflow:scroll;}
.player_list ul{display:flex; width:100%; flex-wrap: wrap; justify-content: space-around;}
.player_list li{width:30%; margin:1.5%; border-radius:2rem; background-color:#0f2e3e;display:flex; flex-direction:column;}
.player_list li .player_name{color: #fff;text-align: center;line-height: 4.4rem;font-size: 2.5rem;letter-spacing:0.3rem ;border-radius: 2rem 2rem 0 0;background-color: #0D2434;}
.player_list li .container{position:relative;}
.player_list li .container .player_position{text-align:center; width:3.36rem; height:8.775rem; top:0.8rem; float:left; position:absolute; right:2.8rem; position:absolute;}
.player_list li .container .forward{background:url(../images/selectplayer/forward.png); background-repeat:no-repeat; float:left;}
.player_list li .container .player_pic{display:flex; flex-direction:column; align-items: center;}
.player_list li .player_button{ font-family:微软雅黑; font-weight: bold; text-align: center;height: 5.6rem;line-height: 5.6rem;font-size: 2rem;letter-spacing: 0.3rem;color: #FEBF00;background-color: #091B27;border-radius: 0 0 2rem 2rem ;}
.player_list ul .player_unselected .player_button{ color: #fdc201;}
.player_list ul .player_selected{ box-shadow: 0px 0px 5px #FFF;}
.player_list ul .player_selected .player_button{ color: #285b74;}
.player_list li .container .player_salary{ position:absolute; bottom:0; background:rgba(7,23,31,0.8); background-repeat:no-repeat; background-position:10px 0.8rem;width:100%; display:flex; align-items:center;height:3.8rem;min-height:3.8rem;}
.player_list li .container .player_salary div{margin-left:1rem;}
.player_list li .container .player_salary .deleted{font-size:1.8rem;text-decoration:line-through;}
.player_list li .container .player_salary .current{font-size:2rem; color:#ffCC00; }
.player_list li .container .player_card_list .current_card{ position:absolute; right:0; bottom:3.8rem;  min-width:10.2rem; max-width:14rem; height:3.25rem; line-height: 3.25rem; right:0px; background:#2485c2; color:#EEE; border-top-left-radius: 15px; border-bottom-left-radius: 15px; font-size:1.8rem;padding-left:1rem; overflow: hidden; text-overflow:ellipsis;white-space:nowrap;}

.player_list li .container .player_card_popup_panel{position:absolute; background-color:#2485c2;  padding:1.5rem 0;  z-index:5; color:#EEE; display:flex; border-radius:1.5rem; width:100%; top:16rem;}
.player_list li .container .player_card_popup_panel ul{list-style:none;background-color:#2485c2;  padding:0; margin:0; display:flex; flex-direction: column;}
.player_list li .container .player_card_popup_panel ul li{padding:0; margin:0;  list-style:none; width:100%; border-radius:0;background-color:#2485c2; font-size:2.4rem; height:4.4rem;display:flex; flex-direction: row; align-items: center;}
.player_list li .container .player_card_popup_panel ul li:nth-child(even){background-color:#1b77af;}
.player_list li .container .player_card_popup_panel ul li input[type='radio']{ width:2rem; height:2rem; margin:0 1.5rem;}

.fix_bottom{margin-top:-3rem;}

.salary_panel{width: 100%;height:18.3rem;background: url(../images/selectplayer/playertaxbg.png) no-repeat top center; background-size: 100%;  position:relative;}
.salary_panel .info{display:flex; top:5.4rem; position:absolute;color:#57b3c7; font-size:2.3rem; width:100%;}
.salary_panel .info .lblSalary{margin:0 2rem;}
.salary_panel .info .lblFreeSalary{position:absolute; left:70%;}
.salary_panel .info .lblMaxSalary{position:absolute; left:90%;}
.salary_panel .salaryBar{ position:absolute; top:10.2rem;width:95%; height:4.7rem; padding:0; margin:0px 2.5%;  background:url(../images/myteam/salary_bg.png) no-repeat; background-size:100% 100%;}
.salary_panel .salaryBar .barValue{ position:absolute; width:95%; height:2.4rem;  margin:1.4rem 0rem 0 0.8rem;  background:url(../images/myteam/salarywarn.png) no-repeat; background-size:100% 100%;}
.salary_panel .salaryBar .icoFootball{ position:absolute; width:4.1rem !important; height:4.1rem !important; top:1rem;  background:url(../images/myteam/football.png)  no-repeat; background-size:cover;}
.salary_panel .lblDescription{position:absolute; top:15.33rem; color:#57b3c7; font-size:1.8rem;text-align: center;width: 100%;}


.root_container{position: relative;}
.team_info{width: 95%;height: 85%;background-color: rgba(255,255,255,.4);z-index: 99;position: absolute;left:0; top:0;right:0;bottom:0;margin:12% 2.5%;display: flex;flex-direction: column;align-items: center; border-radius: 2rem;box-sizing: border-box;}
.team_info .team_info_title{width:100%;height: 7.58rem;background: #0F2E42;text-align: center;position: relative;color: #7091A6;line-height: 7.58rem;font-size: 2.1rem;border-radius: 2rem 2rem 0 0 ;}
.team_info .team_info_title img{position: absolute;top: 1.5rem;right: 1.5rem;}
.team_info .info_list{background-color: rgba(18,65,95,.75);width: 100%;overflow: scroll;height: 75%;}
.team_info .info_list li{display: flex; justify-content: space-around;align-items: center;width: 100%;height: 8rem;}
.team_info .info_list li:nth-child(even){background-color: rgba(15,69,99,.8);}
.team_info .info_list li img{width: 6rem;border-radius: 50%;background-color: #102D3F;}
.team_info .info_list li div{font-size: 2.1rem;}
.team_info .info_list li .coin{display: flex;align-items: center;}
.team_info .info_list li .coin img{width: 3.5rem;margin-right: 1rem;}
.team_info .info_list li .blank{width: 10rem;}

.team_info .salary_info{display: flex;flex-direction: column;height: 10.83rem;background-color: rgba(15,50,70,.8);}
.team_info .salary_info .con{display: flex;justify-content: space-around;height: 50%;align-items: center;}
.team_info .salary_info .con .con_info{display: flex;}
.team_info .salary_info .con .con_info div{font-size: 2.2rem;color: #428A9E;font-weight: bold;}

.team_info .button{flex: 1;background: rgba(17,49,70,.9);text-align: center;font-size: 4rem;color: #EBB505;letter-spacing: .3rem;padding-top: 3rem;border-radius:  0 0 2rem 2rem ;}


.message{position: absolute;top: 0;left: 0;right: 0;bottom: 0;margin: auto;display: flex;flex-direction: column;align-items: center; border-radius: 2rem;width: 90%;height: 25%;background-color: #0D354F;}
.message .message_con{width: 85%;height: 60%;display: flex; font-size: 2.4rem;color: #80A4BB;align-items: center;padding-top: 3rem;}
.message .buttons{height: 30%;background: url(../images/createteam/text_bg.png) no-repeat;background-size:100% 100%;display: flex;justify-content: space-around;align-items: center;}
.message .buttons div{font-size: 2.5rem;color: #FEC000;}
.message .buttons .line{width: 2px;height:100%;background-color: #386D8F;}


.message .message_con1{display: flex;flex-direction: column;width: 80%;height: 60%;font-size: 2.4rem;color: #80A4BB;align-items: center;padding-top: 5rem;box-sizing: border-box;}
.message .message_con1 div:first-child{font-size: 3rem;color: #83A6BE;}
.message .message_con1 div:nth-of-type(2){color: #49697E;margin-top: 1rem;}
.message .button{height: 30%;background: url(../images/createteam/text_bg.png) no-repeat;background-size:100% 100%;margin-top: 3rem;color: #FEC000;text-align: center;font-size: 3rem;letter-spacing: 0.5rem;padding-top: 2rem;box-sizing: border-box;border-radius: 0 0 2rem 2rem;}

.message .success{padding-top: 2rem;}
.message .success img{margin-bottom: 1rem;}
.message .button{height: 30%;background: url(../images/createteam/text_bg.png) no-repeat;background-size:100% 100%;display: flex;justify-content: space-around;margin-top: 3rem;}
.message .button div{font-size: 2.5rem;color: #FEC000;}
.message .button .line{width: 2px;height:100%;background-color: #386D8F;margin-top: -1rem;}


.recharge{position: absolute;top: 0;left: 0;right: 0;bottom: 0;margin: auto;display: flex;flex-direction: column;align-items: center; border-radius: 2rem;width: 82%;height: 35%;background-color: #0D354F;}
.recharge .text{width: 100%;height: 20%;text-align: center;font-size: 2.3rem;color: #8CB1C8;padding-top: 3rem;}
.recharge .recharge_list{width: 100%; flex: 1;}
.recharge .recharge_list li{height: 25%;text-align: center;font-size: 2.6rem;color: #ffc000;display: flex;align-items: center;justify-content: center;box-sizing: border-box;border-bottom: 0.2rem solid #3B7195;}
.recharge .recharge_list li:first-child{border-top: 0.2rem solid #3B7195;}
.recharge .recharge_list li:last-child{border-radius: 0 0 2rem 2rem;}
