.address_content{padding: 3rem 5rem;height: 82%;}


.cons{width: 100%;display: flex;flex-direction: column; margin-bottom: 2rem;}
.cons div{color: #8DB1C8;font-size: 2.6rem;}
.cons input{width: 100%;height: 6rem;min-height:6rem;font-size: 4rem;background-color: #0E3753;outline: none;border: none;border-radius:2rem ;margin-top: 2rem;color: #fff;padding: 0 2rem;box-sizing: border-box;}

.cons #address{height: 20rem;}


#bottom_button{background:url(../images/createteam/text_bg.png) no-repeat top center;background-size: 100% 100%;text-align: center;font-size: 2.7rem;color: #8E908F;height: 8.45rem;line-height: 8.4rem;color: #FFC000;letter-spacing: .5rem;}
