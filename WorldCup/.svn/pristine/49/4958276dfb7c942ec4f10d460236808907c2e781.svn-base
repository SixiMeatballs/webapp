html,body{
    width: 750px;
    overflow-x: hidden; padding: 0; margin: 0;
}

.left{float: left;}
.right{float:right;}
.clear{clear: both;}
.none{
    display: none;
}


.container1{
    background-image: url(../images/index/bg.png);
    background-size:100% 100%;
    background-repeat: no-repeat;
    overflow:visible;
    width: 750px;
    height: 1334px;
}

.header div{ margin-top: 20px;}

.header .back{
    margin-left: 50px;
}

.container1 .header .help{
    margin-right: 50px;
}


.main img{
    width: 750px; height: 824px;
}

.nav{ margin-top: 25px;}

.nav a{
    width: 25%; display: block; float: left;
    text-align: center;
}

.nav img{
   width: 121px;
   height: 141px;   
}

.containerselectround{
    background-image: url(../images/selectround/bg.png);
    background-size:100% 100%;
    background-repeat: no-repeat;
    overflow:visible;
    width: 750px;
    height: 1334px;
}

.header .title{
    color: white; font-size: 38px; margin-left: 150px; padding-top: 5px;
}

.round{ margin-left: 50px; width: 650px; margin-top: 20px;}

.round .rounddetail{ width: 130px; color: white;}

.round .rounddetail p::nth-of-type(0){ font-size: 18px;}
.round .rounddetail p::nth-of-type(1){ font-size: 14px;}




.containerselectplayer{
    background-image: url(../images/index/bg.png);
    background-size:100% 100%;
    background-repeat: no-repeat;
    overflow:hidden;
    width: 750px;
    height: 1334px;
}

.headerselectplayer div{
    margin-top: 33px;
    margin-bottom: 0px;
}

.headerselectplayer .back{
    margin-left: 22px;
}

.headerselectplayer .title{
    color: white;
    line-height: 53px;
    font-size: 34px; 
    width: 650px;
    text-align: center;
}

.selectedplayer{
    width: 750px; height: 200px;
    background-image: url(../images/selectplayer/playerbg.png);
    background-size:100% 100%;
    background-repeat: no-repeat;
    overflow:visible;
}

.selectedplayer .number{
    width: 137px; height: 112px;
    color: white; text-align: center; line-height: 50px;
}

.selectedplayer .selectedphoto
{
    width: 613px; height: 112px; overflow: hidden;
}

.selectedplayer .selectedphoto .playerphoto1{
    background-image: url(../images/selectplayer/selectplayer.png);
    background-size:86px 86px;
    background-repeat: no-repeat;
    background-position-y: center;
    width: 86px; height: 112px;
    margin-left: 15px;

}

.selectedplayer .teamlist{
    width: 750px; height: 78px; overflow: hidden;
    line-height: 78px; color: white;
    font-size: 32px;
}

.selectedplayer .teamlist .team{
    width: 20%; text-align: center;
}

.selectedplayer .teamlist .selected{
    color:#9e8021;
    font-weight: bold;
}

.playerlist{
    background-color: #114c6b; height: 712px; padding-top: 25px;
    overflow-y: hidden;
}

.playerlist .player{
    background-image: url(../images/selectplayer/playerbg2.png);
    background-size:230px 300px;
    background-repeat: no-repeat;
    width: 230px; height: 300px;
    margin-left: 15px;
    margin-bottom: 15px;
}

.playerlist .player .playername{
   width: 230px;
   color: white;
   text-align: center;
   height: 46px;
   line-height: 46px;
}

.playerlist .player .playerimg{
    width: 230px; height: 200px;
}

.playerlist .player .playerimg .gzm{
    background-image: url(../images/selectplayer/playervaluebg.png);
    background-size:230px 44px;
    background-repeat: no-repeat;
    height: 44px;
    position: relative;
  left: 0px;
  top: -44px;
}

.playerlist .player .playerimg .gzm img{
    margin-left: 10px;
    margin-right: 15px;
}

.playerlist .player .playerimg .gzm .value{
    color: #f2ba00;
}


.playerlist .player .select{
    color: #f2ba00; font-weight: bold; width: 230px; text-align: center; line-height: 54px;

}

.playermoney{
    background-image: url(../images/selectplayer/gzmbg.png);
    background-size:750px 224px;
    background-repeat: no-repeat;
    width: 750px; height: 195px;
    margin-top: -30px;
    margin-bottom: 0;
    clear: both;
}

.playermoney .moneyline1{
    height: 96px;
}

.playermoney .moneyline1 div{
    margin-top: 25px; line-height: 74px;  color: #265764; font-weight: bold;
}

.playermoney .moneyline3 {
    line-height: 74px;  color: #265764; font-weight: bold; padding-left: 30px; 
}

.playermoney .moneyline1 .col1{
    width: 380px; padding-left: 30px;
}

.playermoney .moneyline1 .col2{
    width: 233px;
}

.playermoney .moneyline2{
    height: 63px;
    background-image: url(../images/selectplayer/kuang.png);
    background-size:710px 62px;
    background-repeat: no-repeat;
    background-position-y: center;
    background-position-x: center;
    width: 710px;    
    margin-left: 10px;
}

.playermoney .moneyline2 .kuang{
    height: 63px;
    background-image: url(../images/selectplayer/yellow.png);
    background-size:690px 32px;
    background-repeat: no-repeat;
    background-position-y: 20px;
    background-position-x: center;
    width: 690px;
    margin-left: 10px;
}



.playermoney .moneyline2 .kuang .col3{
   height: 32px; background-color: #0d222c; margin-top: 21px;
   border-radius:25px;
}
.playermoney .moneyline2 .kuang .col3 img{
    margin-left: -23px;
}
.playermoney .moneyline3{
    height: 73px;
}

.containerselectplayer .next{
    height: 106px; margin-top: 38px;
}
img[name='icoFootball']{margin-top:-5px;}